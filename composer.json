{
  "name": "noesis/dianoia",
  "description": "The core of the Noesis Framework",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Noesis\\Dianoia\\": "src/",
      "Test\\Mock\\": "tests/mocks/"
    },
    "files": [
      "./src/Support/functions.php"
    ]
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "laminas/laminas-servicemanager": "^3.15",
    "laminas/laminas-httphandlerrunner": "^2.1",
    "laminas/laminas-diactoros": "^2.13",
    "laminas/laminas-di": "^3.9",
    "php-di/php-di": "^6.4",
    "laminas/laminas-config": "^3.7",
    "league/route": "^5.1",
    "aura/session": "^2.1",
    "monolog/monolog": "^3.1",
    "laminas/laminas-eventmanager": "^3.5",
    "maximebf/debugbar": "^1.18",
    "filp/whoops": "^2.14",
    "illuminate/support": "^9.21",
    "illuminate/database": "^9.21",
    "illuminate/events": "^9.22"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "pestphp/pest": "^v1.21",
    "squizlabs/php_codesniffer": "^3.7",
    "pestphp/pest-plugin-mock": "^v1.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
